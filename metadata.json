{
    "name": "rl-bareos",
    "version": "1.0.11",
    "author": "Kjetil Torgrim Homme <kjetil.homme@redpill-linpro.com>",
    "summary": "Manage Bareos clients and jobs",
    "license": "GPL-3.0+",
    "source": "https://github.com/kjetilho/rl-bareos.git",
    "project_page": "https://forge.puppetlabs.com/rl/bareos",
    "issues_url": "https://github.com/kjetilho/rl-bareos/issues",
    "operatingsystem_support": [
        {
            "operatingsystem": "CentOS",
            "operatingsystemrelease": [
                "7"
            ]
        },
        {
            "operatingsystem": "Debian",
            "operatingsystemrelease": [
                "8"
            ]
        },
        {
            "operatingsystem": "Ubuntu",
            "operatingsystemrelease": [
                "14.04"
            ]
        },
        {
            "operatingsystem": "windows",
            "operatingsystemrelease": [
                "2012 R2",
                "NOTE: facterdb and/or rspec-puppet-fact does not support Windows yet"
            ]
        }
    ],
    "dependencies": [
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">= 4.10.0"
        }
    ],
    "tags": ["backup", "bacula", "bareos"],
    "data_provider": "hiera"
}
